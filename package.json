{
  "name": "DEALER_PORTAL",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^6.8.4",
    "@sap/cds-hana": "^2",
    "@sap-cloud-sdk/core": "^1.54.2",
    "nodemailer": "^6.9.7",
    "core-util-is": "^1.0.3",
    "@sap/hana-client": "^2.18.27",
    "@sap-cloud-sdk/http-client":"^3.4.0",
    "@sap/hdbext": "^7.7.5",
    "express": "^4.18.2",
    "hdb": "^0.19.5",
    "passport": "^0.6.0",
    "sap-hdbext-promisfied": "^2.202306.2"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "cds": {
    "requires": {
      "db": "hana",
      "ZIDL_CUSTOMER_REG_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/ZIDL_CUSTOMER_REG_SRV",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "destination": "iDeal_S4HANA",
          "path": "/sap/opu/odata/sap/ZIDL_CUSTOMER_REG_SRV/",
          "forwardAuthToken": true,
          "timeout": 100000
        },
        "[production]": {
          "credentials": {
            "destination": "iDeal_S4HANA",
            "path": "/sap/opu/odata/sap/ZIDL_CUSTOMER_REG_SRV"
          }
        }
      },
      "ZMDG_CUSTOMER_REG_SRV": {
        "kind": "rest",
        "model": "srv/external/ZMDG_CUSTOMER_REG_SRV",
        "csrf": true,
        "csrfInBatch": true,
        "credentials": {
          "destination": "iDeal_S4HANA",
          "path": "/sap/opu/odata/sap/ZMDG_CUSTOMER_REG_SRV/"
        },
        "[production]": {
          "credentials": {
            "destination": "iDeal_S4HANA",
            "path": "/sap/opu/odata/sap/ZMDG_CUSTOMER_REG_SRV"
          }
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    }
  },
  "devDependencies": {
    "@sap/hdi-deploy": "4.8.2"
  }
}